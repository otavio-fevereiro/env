

# {mark} START IT-ENG JAMF SETUP ZSHRC
source $HOME/.nurc
export NU_HOME="$HOME/dev/nu"
export MMR="$NU_HOME/dev/nu/mini-meta-repo"
export ENV="$NU_HOME/dev/nu/env"
export GOPATH="${NU_HOME}/go"
export PATH="$GOPATH/bin:${PATH}"
export MONOREPO_ROOT="$NU_HOME/mini-meta-repo"
export PATH="$PATH:$MONOREPO_ROOT/monocli/bin"
export FLUTTER_ROOT="$HOME/sdk-flutter/"
export PATH="$PATH:$FLUTTER_ROOT/bin"
export PATH="$PATH:$HOME/.pub-cache/bin"
export FLUTTER_PLATFORM_CLIENT_ENV=staging
eval "$(rbenv init -)"
# {mark} END IT-ENG JAMF SETUP ZSHRC

alias hover="source $ENV/hover.sh"
alias stormguild="source $ENV/stormguild-mock.sh"
alias flutter-d="source $ENV/flutter-desktop.sh"
alias flutter-d-s="open -a Terminal $ENV/stormguild-mock.sh &&
                   open -a Terminal $ENV/flutter-desktop.sh"
alias cid="nu cus cpf 39570682825"
alias update-credentials="source $ENV/update-credentials.sh"


export PATH="/opt/homebrew/bin:$PATH"
